#SET(%LineNext, 'EQUATE(' & FORMAT(%CountEquate,@N03) & ')')
#SET(%LineCmnt, FORMAT(%CountEquate,@N03) & ': ' & %RYB111Descr)
%[%zNames111]LineWork %[%zItems111-%zNames111]NULL %LineNext #<!:%LineCmnt
#!
    #ENDFOR
  #RESUME
!endregion EQUATE
!
!region VALUE
!                                         1234567890123456
  #SET(%CountEquate, 0)
  #SUSPEND
    #FOR (%RYB111Includes)
      #SET(%CountEquate, %CountEquate + 1)
