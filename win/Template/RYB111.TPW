    #FOR (%RYB111Includes)
      #SET(%CountEquate, %CountEquate + 1)
#SET(%LineWork, %RYB111TableDescr & %RYB111Code)
#SET(%LineNext, 'EQUATE(''' & CLIP(%RYB111Descr) & ''')')
